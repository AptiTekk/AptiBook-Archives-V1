<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd">

    <!-- Migrates Properties from the old Ordinal based column to Name based. Removes old example properties. -->
    <changeSet id="3__Migrate_Properties_From_Ordinal_to_Name__7_25_2016" author="MitchTalmadge">
        <delete tableName="Property"/>
        <renameColumn tableName="Property" oldColumnName="propertyKey" newColumnName="propertyKey"
                      columnDataType="VARCHAR(255)"/>
    </changeSet>

</databaseChangeLog>